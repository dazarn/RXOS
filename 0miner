do
sleep 100
done
fi

if [ $COIN == "MUSIC" ]
then

if [ $GENOILorCLAYMORE == "GENOIL-CURRENTLY-NOT-SUPPORTED" ]
then
HCD='/home/m1/eth/Genoil/ethminer'

MUSICADDR="$MUSIC_ADDRESS/$MUSIC_WORKER"
until $HCD -S $MUSIC_POOL -O $MUSICADDR:x -G
   do
